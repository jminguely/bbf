<head>
    <title>{{ wp_title }}{{ site.name }}</title>
    <meta charset="{{ site.charset }}" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="format-detection" content="telephone=no">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="pingback" href="{{ site.pingback_url }}" />
    <link rel="apple-touch-icon" sizes="180x180" href="{{ theme.link }}/build/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ theme.link }}/build/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ theme.link }}/build/favicons/favicon-16x16.png">
    <link rel="manifest" href="{{ theme.link }}/build/favicons/site.webmanifest">
    <link rel="mask-icon" href="{{ theme.link }}/build/favicons/safari-pinned-tab.svg" color="#ca0000">
    <link rel="shortcut icon" href="{{ theme.link }}/build/favicons/favicon.ico">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-config" content="{{ theme.link }}/build/favicons/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
    {{function('wp_head')}}
</head>
